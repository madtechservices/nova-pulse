{
    "name": "madtechservices/nova-pulse",
    "description": "A Laravel Nova tool to embed Laravel Pulse in an iframe.",
    "keywords": [
        "laravel",
        "pulse",
        "laravel pulser",
        "debugging",
        "nova 4",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "laravel/nova": "^4.0",
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Madtechservices\\NovaPulse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madtechservices\\NovaPulse\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
